    <property name="styleSheet">
     <string notr="true">/* Style du formulaire */
QWidget {
    background-color: #f4f4f4; /* Fond gris clair moderne */
}

/* Style des champs de saisie (ID, Formateur, Lieu, etc.) */
QLineEdit {
    background-color: white;
    border: 2px solid #cccccc;
    border-radius: 6px;
    padding: 5px;
    font-size: 14px;
}

QLineEdit:focus {
    border: 2px solid #3498db; /* Bleu moderne au focus */
}

/* Style des DateEdit */
QDateEdit {
    background-color: white;
    border: 2px solid #cccccc;
    border-radius: 6px;
    padding: 3px;
    font-size: 14px;
}

QDateEdit::drop-down {
    border: none;
}

/* Style du QSlider (Prix) */
QSlider::groove:horizontal {
    background: #ddd;
    height: 6px;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #3498db;
    width: 14px;
    height: 14px;
    border-radius: 7px;
    margin: -4px 0;
}

/* Style des labels */
QLabel {
    font-size: 14px;
    font-weight: bold;
    color: #2c3e50; /* Gris foncé pour un effet pro */
}
</string>
